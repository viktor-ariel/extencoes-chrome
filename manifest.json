{
    "manifest_version" : 2,
    "name":"Download numbers",
    "version":"1.0",
    "description":"baixar numeros do whatsapp",

    "backgroud":{
        "scripts":["src/backgroud.js"],
        "persistent":false
    },
    
    
    "browser_action": {
      
        "default_title":"Download Numbers",
        "default_popup": "src/index.html"
    },
    "content_scripts":[
        {
            "js":[
                "src/main.js"
            ],
            "matches":[
                "https://web.whatsapp.com/*"
            ]
        }
    ]
}